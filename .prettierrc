{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "printWidth": 100,
  "semi": false,
  "bracketSameLine": false,
  "importOrder": [
    "reflect-metadata",
    "^react$",
    "^mobx",
    "ui-types",
    "^@core/(.*)$",
    "uiCore/*",
    "^@mui/",
    "^@.",
    "(\\w+\\.)+(?!css|ts|js)",
    "../",
    ".",
    "\\.[css|ts|js]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx"]
}
